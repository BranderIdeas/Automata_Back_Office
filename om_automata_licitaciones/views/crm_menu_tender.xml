<odoo>
    <data>

        <!-- tree -->
        <record model="ir.ui.view" id="crm_menu_tender_view">
            <field name="name">crm_menu_tender.view</field>
            <field name="model">om_automata_licitaciones.crm_menu_tender</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <!-- search -->
        <record model="ir.ui.view" id="om_automata_licitaciones.crm_menu_tender_search">
            <field name="name">om_automata_licitaciones.crm_menu_tender.search</field>
            <field name="model">om_automata_licitaciones.crm_menu_tender</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                </search>
            </field>
        </record>

        <!-- form -->
        <record model="ir.ui.view" id="oe_agencia_viajes_crm_menu_tender">
            <field name="name">om_automata_licitaciones.crm_menu_tender.form</field>
            <field name="model">om_automata_licitaciones.crm_menu_tender</field>
            <field name="arch" type="xml">
                <form string="Licitaciones">
                    <sheet>
                        <div class="oe_title">
                            <h3>
                                <field name="name"/>
                            </h3>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="om_automata_licitaciones_crm_menu_tender">
            <field name="name">Licitaciones</field>
            <field name="res_model">om_automata_licitaciones.crm_menu_tender</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Licitaciones
                </p>
            </field>
        </record>

        <!-- menu item -->
        <menuitem id="crm_menu_tender"
                  name="Licitaciones"
                  parent="crm.crm_menu_root"
                  action="om_automata_licitaciones_crm_menu_tender"
                  sequence="4"/>
        
    </data>
</odoo>